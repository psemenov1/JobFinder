{
  "manifest_version": 3,
  "name": "Job Finder Extension",
  "version": "1.0",
  "description": "Find jobs based on your preferences and skills.",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["https://api.adzuna.com/*"],
  "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  },
  "background": {
      "service_worker": "background.js"
  }
}
